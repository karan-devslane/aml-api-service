{
  "name": "aml-service",
  "version": "1.0.0",
  "description": "aml Service",
  "main": "src/app.ts",
  "private": true,
  "engines": {
    "node": "20.11.1",
    "npm": "10.5.0"
  },
  "scripts": {
    "build": "tsc",
    "build:prod": "npx --node-options=\"--max-old-space-size=2048\" tsc",
    "start": "node --inspect dist/app.js",
    "prestart": "npm run build",
    "start:dev": "ts-node-dev --inspect --respawn --transpile-only --env-file=.env  --watch './src/**/*.ts' src/app.ts",
    "start:dev:win": "ts-node-dev --respawn --transpile-only --env-file=.env --watch './src/**/*.ts' src/app.ts",
    "lint": "eslint .",
    "test": "mocha --exit",
    "lint:fix": "eslint --fix .",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "prepare": "husky install",
    "gen:migration": "./migration-generate.sh",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.637.0",
    "@aws-sdk/s3-request-presigner": "^3.637.0",
    "ajv": "^8.17.1",
    "app-root-path": "^3.1.0",
    "bcrypt": "^5.1.1",
    "connect-pg-simple": "^10.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cron": "^3.2.1",
    "csurf": "^1.11.0",
    "express": "^5.0.0-beta.3",
    "express-fileupload": "^1.5.1",
    "express-rate-limit": "^7.4.1",
    "express-session": "^1.18.1",
    "http-status": "^1.7.4",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "papaparse": "^5.5.1",
    "sequelize": "^6.37.3",
    "sequelize-cli": "^6.6.2",
    "sequelize-typescript": "^2.1.6",
    "uuid": "^9.0.1",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/chai": "^4.3.3",
    "@types/chai-spies": "^1.0.3",
    "@types/connect-pg-simple": "^7.0.3",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/csurf": "^1.11.5",
    "@types/express": "^4.17.21",
    "@types/express-fileupload": "^1.5.1",
    "@types/express-session": "^1.18.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/lodash": "^4.17.0",
    "@types/mime-types": "^2.1.4",
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.14.14",
    "@types/papaparse": "^5.3.15",
    "@types/pg": "^8.11.8",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "chai-spies": "^1.1.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^17.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-security": "^2.1.1",
    "husky": "^8.0.0",
    "mocha": "^10.7.0",
    "nyc": "^17.0.0",
    "pg": "^8.12.0",
    "prettier": "^3.2.5",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  }
}
